				var kkeys = [], kk = "38,38,40,40,37,39,37,39,66,65";
				$(document).keydown(function(e) {
					kkeys.push( e.keyCode );
					if ( kkeys.toString().indexOf( kk ) >= 0 ) {
						$(document).unbind('keydown',arguments.callee);
